{
  "name": "firesphere/rangefield",
  "description": "Range selector for SilverStripe",
  "type": "silverstripe-vendormodule",
  "license": "BSD-3-Clause",
  "keywords": [
    "RangeField"
  ],
  "authors": [
    {
      "name": "Simon Erkelens",
      "email": "simonerkelens@silverstripe.com"
    }
  ],
  "require": {
    "silverstripe/framework": "^4.0"
  },
  "require-dev": {
    "scriptfusion/phpunit-immediate-exception-printer": "^2.0",
    "phpunit/phpunit": "^5.7",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "extra": {
    "installer-name": "rangefield",
    "branch-alias": {
      "1.x-dev": "1.0.x-dev",
      "dev-master": "1.x-dev"
    },
    "expose": [
      "client/dist"
    ],
    "autoload-dev": {
      "psr-4": {
        "Firesphere\\RangeField": "src"
      }
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
